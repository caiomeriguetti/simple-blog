      ProxyPreserveHost On
      ProxyVia On

      <Proxy balancer://clusterApi>
        BalancerMember http://localhost:9088 route=route1
        BalancerMember http://localhost:9089 route=route2
        ProxySet lbmethod=bybusyness
      </Proxy>
      <Location />
