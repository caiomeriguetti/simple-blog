#
# UI LOAD BALANCER
#

Listen 9090
<VirtualHost *:9090>
  ServerName ec2-54-187-107-64.us-west-2.compute.amazonaws.com
  ServerName ec2-54-187-107-64.us-west-2.compute.amazonaws.com

  <IfModule mod_proxy_http.c>
      ProxyRequests Off
      ProxyTimeout 300
      ProxyPreserveHost On
      ProxyVia On

      <Proxy balancer://clusterUi>
        BalancerMember http://localhost:9086 route=route1
        BalancerMember http://localhost:9087 route=route2
        ProxySet lbmethod=bybusyness
      </Proxy>
      <Location />
        Order allow,deny
        Allow from all
        ProxyPass balancer://clusterUi/ stickysession=PHPSESSIONID
      </Location>

  </IfModule>
</VirtualHost>


